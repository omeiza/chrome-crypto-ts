{
  "name": "Chrome Crypto",
  "version": "1.1.0",
  "default_locale": "en",
  "manifest_version": 3,
  "description": "Replace your new tab page with a list of popular cryptocurrencies and their current prices.",
  "homepage_url": "https://github.com/omeiza/chrome-crypto-ts",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/page-override/index.html"
  }
}